{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "College",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "Unique identifier for the college"
    },
    "name": {
      "type": "string",
      "description": "Name of the college"
    },
    "location": {
      "type": "string",
      "description": "Location of the college"
    },
    "coursesOffered": {
      "type": "array",
      "items": {
        "type": "string",
        "description": "List of courses offered by the college"
      }
    },
    "facilities": {
      "type": "array",
      "items": {
        "type": "string",
        "description": "List of facilities available at the college"
      }
    },
    "cutOff": {
      "type": "number",
      "description": "Cut-off percentage for admission"
    },
    "mediumOfInstruction": {
      "type": "string",
      "description": "Medium of instruction used in the college"
    }
  },
  "required": ["id", "name", "location", "coursesOffered", "facilities", "cutOff", "mediumOfInstruction"]
}